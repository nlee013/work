	function gd_dailyDeliveryTimer(duration,nextDuration,contentsId) {
		
        var timer = duration;
        var days,hours, minutes, seconds, addText, deliveryType, pres;
		
		var meshDeliveryPossible;

		if(contentsId == 'todayDeliveryTime'){
			addText = nextDuration + " 오후 09시 도착 예정" + ' / 서울, 경기 일부';
			deliveryType = 'today';
			meshDeliveryPossible = 'y';
		}
		else if(contentsId == 'normalDeliveryTime'){
			addText = nextDuration + " 출고 예정";
			deliveryType = '';
			meshDeliveryPossible = 'n';
		}

        var interval = setInterval(function(){
            days	= parseInt(timer / 86400, 10);
            hours	= parseInt(((timer % 86400 ) / 3600), 10);
            minutes = parseInt(((timer % 3600 ) / 60), 10);
            seconds = parseInt(timer % 60, 10);
			pres = '';

            if(days ==0) {
                //$('.time-day-view').hide();
            } else {
                days 	= days < 10 ? "0" + days : days;
                //$('#displayTimeSaleDay').text(days);
				pres = days + "일";
            }

            hours 	= hours < 10 ? "0" + hours : hours;
            minutes = minutes < 10 ? "0" + minutes : minutes;
            seconds = seconds < 10 ? "0" + seconds : seconds;

            $('#'+contentsId).text(pres + " " + hours + ":"+minutes + ":"+seconds + " 내 주문시, " + addText);

            //$("#displayTimeSale").show();

			
            if (--timer < 0) {

				var params = {
					mode: 'get_delivery_time',
					deliveryPossible: meshDeliveryPossible,
					'deliveryType': deliveryType,
				};

				jQuery.ajax({
					url : '../share/mesh_ps.php',
					type : 'POST',
					dataType:'json',
					data: params,
					async: false,					
					success : function(result) {	
						
						timer = result.nextDuration;	
						
						if(contentsId == 'todayDeliveryTime') addText = result.nextDate + " 오후 09시 도착 예정" + ' / 서울, 경기 일부';
						else if(contentsId == 'normalDeliveryTime') addText = result.nextDate + " 출고 예정";

					}

				});
					
				/*
                timer = nextDuration;
                //clearInterval(interval);

				if(contentsId == 'todayDeliveryTime') addText = "내일 오후 09시까지 도착보장";
				else if(contentsId == 'normalDeliveryTime') addText = "03/02 출고 예정";
				*/
            }
        }, 1000);
    }

